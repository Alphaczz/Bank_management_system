<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>BankManagement</display-name>
	<welcome-file-list>


		<welcome-file>transfer.jsp</welcome-file>
		<welcome-file>transactionHistory.jsp</welcome-file>
		<welcome-file>customerDashboard.jsp</welcome-file>
		<welcome-file>customerLogin.jsp</welcome-file>
		<welcome-file>employeeDashboard.jsp</welcome-file>
		<welcome-file>employeeLogin.jsp</welcome-file>
		<welcome-file>landingPage.jsp</welcome-file>
		<welcome-file>loanApp.jsp</welcome-file>
		<welcome-file>transactionManagement.jsp</welcome-file>

	</welcome-file-list>

	<servlet>
		<servlet-name>TransactionServlet</servlet-name>
		<servlet-class>TransactionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TransactionServlet</servlet-name>
		<url-pattern>/TransactionServlet</url-pattern>
	</servlet-mapping>

</web-app>